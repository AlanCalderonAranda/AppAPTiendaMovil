<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InicioActivity">

    <LinearLayout
        android:id="@+id/linearLayout12"
        android:layout_width="146dp"
        android:layout_height="362dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="51dp"
        android:layout_marginBottom="153dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/linearLayout16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout15"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/ivTienda"
            android:layout_width="143dp"
            android:layout_height="128dp"
            app:srcCompat="@drawable/tienda" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="\nTienda Online\n" />

        <ImageView
            android:id="@+id/ivSoporte"
            android:layout_width="143dp"
            android:layout_height="128dp"
            app:srcCompat="@drawable/telefono" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="\nSoporte Telefonico\n" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout16"
        android:layout_width="146dp"
        android:layout_height="362dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="51dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="153dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/linearLayout12"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout15">

        <ImageView
            android:id="@+id/ivWeb"
            android:layout_width="143dp"
            android:layout_height="128dp"
            app:srcCompat="@drawable/web" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="\nPagina web\n" />

        <ImageView
            android:id="@+id/ivChat"
            android:layout_width="143dp"
            android:layout_height="128dp"
            app:srcCompat="@drawable/chat" />

        <TextView
            android:id="@+id/tvSoporte"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="\nSoporte Online\n" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout15"
        android:layout_width="331dp"
        android:layout_height="83dp"
        android:layout_marginTop="82dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="82dp"
            android:layout_height="83dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/iconopersona" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="Bienvenid@"
            android:textStyle="normal|bold" />

        <TextView
            android:id="@+id/tvNombrePersona"
            android:layout_width="106dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="#9C27B0"
            android:textSize="24sp"
            android:textStyle="bold|italic" />
    </LinearLayout>

    <Button
        android:id="@+id/btnLogout"
        android:layout_width="99dp"
        android:layout_height="43dp"
        android:layout_marginStart="267dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="45dp"
        android:backgroundTint="#FF0505"
        android:text="Logout"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>